    "@sequelize/core": "^7.0.0-alpha.37",
    "@types/lodash": "^4.14.201",
    "colby": "^0.2.2",
    "electron": "^29.1.5",
    "express": "^4.19.2",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.45.0",
    "next": "14.1.1",
    "prompts": "^2.4.2",
    "react": "^18",
    "react-dom": "^18",
    "sqlite3": "^5.1.7",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
